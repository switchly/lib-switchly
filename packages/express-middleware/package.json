{
  "name": "@cornerstone-digital/switchly-express-middleware",
  "version": "0.1.0-alpha.17",
  "main": "dist/package.cjs.js",
  "module": "dist/package.esm.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "start": "cross-env TS_NODE_FILES=true ts-node --project ../../tsconfig.json ./src/playground/index.ts",
    "json-server": "json-server ./.jsondb/data.json",
    "playground": "cross-env DEBUG=Switchly* nodemon --exec yarn start",
    "app:start": "parcel ./src/web-app/index.html",
    "app:build": "parcel build ./src/web-app/index.html -d ./dist/app",
    "prestart": "yarn app:build",
    "prebuild": "rm -rf dist",
    "build": "bili --config ../../bili.config.ts",
    "postbuild": "yarn app:build",
    "watch": "bili --watch --config ../../bili.config.ts"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.17.1",
    "@types/express": "^4.17.2",
    "@types/flat": "^0.0.28",
    "@types/helmet": "^0.0.45",
    "@types/jest": "^24.0.21",
    "@types/lodash.camelcase": "^4.3.6",
    "@types/mongodb": "^3.3.9",
    "@types/node": "^12.12.5",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-config": "^5.0.1",
    "@types/react-router-dom": "^5.1.2",
    "@types/uuid": "^3.4.6",
    "@types/webpack": "^4.39.8",
    "@types/webpack-dev-middleware": "^2.0.3",
    "@types/webpack-hot-middleware": "^2.16.5",
    "bili": "^4.8.1",
    "cross-env": "^6.0.3",
    "jest": "^24.9.0",
    "jest-config": "^24.9.0",
    "json-server": "^0.15.1",
    "node-sass": "^4.13.0",
    "nodemon": "^1.19.4",
    "parcel": "^1.12.4",
    "rimraf": "^3.0.0",
    "rollup-plugin-typescript2": "^0.24.3",
    "serve": "^11.2.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^3.7.2",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  },
  "dependencies": {
    "@cornerstone-digital/loggerhead": "^1.4.6",
    "@material-ui/core": "^4.6.1",
    "@material-ui/icons": "^4.5.1",
    "@nano-sql/adapter-redis": "^2.0.7",
    "@nano-sql/core": "^2.3.7",
    "ajv": "^6.10.2",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "event-source-polyfill": "^1.0.9",
    "express": "^4.17.3",
    "flat": "^5.0.0",
    "helmet": "^3.21.2",
    "mongodb": "^3.3.3",
    "mongodb-client-encryption": "^0.3.1",
    "node-sass-middleware": "^0.11.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-express-middleware": "^3.1.3",
    "react-router": "^5.1.2",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.1.2",
    "react-ssr": "^2.0.0-beta.5",
    "reflect-metadata": "^0.1.13",
